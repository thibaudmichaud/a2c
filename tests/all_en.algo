algorithm function coucou:int
local parameters
  boolean a
  int b
variables
  char c
  int i, n
begin

n <- 10

for i <- 1 to n do
  b <- b + 1
end for

while b < 10 do
  b <- b + 1
end while

do
  a <- true
while b < 20

if b < 10 then
  a <- false
else
  b <- 1
end if

caseof n do
  1: b <- 1
  2: b <- 2
end caseof

return 0

end algorithm function coucou

variables
  boolean variable_globale
begin

  coucou(true, 1)

end
