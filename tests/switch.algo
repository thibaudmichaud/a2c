algorithme fonction testswitch : entier
parametres locaux
  entier n
variables
  entier a, b
debut
  a <- 0
  b <- 0
  selon n faire
    1, 2 : b <- 1
    3 : b <- 2
  autrement b <- 0
  fin selon
  retourne b
fin algorithme fonction testswitch

variables
  booleen variable_globale
debut
  ecrire(testswitch(2))
fin
